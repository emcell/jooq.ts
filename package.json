{
  "name": "jooq.ts",
  "version": "0.1.16",
  "license": "MIT",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "homepage": "https://github.com/emcellsoft/jooq.ts",
  "repository": "https://github.com/emcellsoft/jooq.ts",
  "scripts": {
    "start": "node dist/main.js",
    "watch": "tsc -w",
    "lint": "eslint src/**/*.ts",
    "start:prod": "node dist/main",
    "codegen:graphql": "graphql-codegen --config codegen.yml",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn lint --max-warnings 0 &&  tsc -p tsconfig.build.json ",
    "test": "rimraf dist && yarn jest --runInBand",
    "migrate": "export $(cat .env | xargs) && yarn graphile-migrate"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/pg": "^7.14.9",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "graphile-migrate": "^1.0.2",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.0",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "pg": "^8.5.1"
  }
}
